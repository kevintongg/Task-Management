# Supabase Configuration
# Copy this file to .env.local and replace with your actual values
# Get these values from your Supabase project dashboard at https://app.supabase.com

# Your Supabase project URL (found in Settings > API)
VITE_SUPABASE_URL=your_supabase_project_url_here

# Your Supabase anon/public key (found in Settings > API)
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Application environment
VITE_APP_ENV=development

# Optional: Application version (automatically set by CI/CD)
# VITE_APP_VERSION=1.0.0

# Optional: Sentry DSN for error tracking
# VITE_SENTRY_DSN=your_sentry_dsn_here

# Optional: Google Analytics tracking ID
# VITE_GA_TRACKING_ID=your_ga_tracking_id_here

# Optional: Feature flags
# VITE_ENABLE_ANALYTICS=false
# VITE_ENABLE_DEBUG=true

# Development only: Enable detailed error messages
# VITE_DEBUG_MODE=true
